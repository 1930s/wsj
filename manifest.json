{
    "name": "free-wsj",
    "description": "bypass wsj paywall",
    "version": "1.1",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "*://*.wsj.com/*"
    ]
}
