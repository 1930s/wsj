{
  "name": "free-wsj",
  "description": "Bypass the WSJ paywall",
  "version": "1.2",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": ["*://*.wsj.com/*"],
      "css": ["hide_popup.css"]
    }
  ],
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.wsj.com/*"
  ]
}
